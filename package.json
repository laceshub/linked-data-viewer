    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "lint-staged": "7.1.0",
    "lodash": "^4.17.21",
    "mocha": "5.1.1",
    "prettier": "^2.5.1",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.3.2",
    "remap-istanbul": "0.11.1",
    "style-loader": "0.21.0",
