{
  "name": "laces-linked-data-viewer",
  "version": "3.5.0",
  "description": "Laces Linked Data Viewer",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "start": "webpack-dev-server",
    "start-test": "webpack-dev-server --env.tests=true",
    "test": "karma start karma.conf.js",
    "build": "webpack --env.prod=true",
    "postbuild": "npm run build-war",
    "build-dev": "webpack",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage-remap && npm run coverage-report-html",
    "coverage-remap": "remap-istanbul -i reports/coverage/coverage.json -o reports/coverage/coverage.json -t json",
    "coverage-report-html": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage html",
    "precommit": "lint-staged",
    "autoformat": "prettier --write \"**/*.{ts,tsx,js,json,css,less}\"",
    "tslint": "tslint --project . --format verbose",
    "tslint-fix": "tslint --project . --format verbose --fix",
    "build-war": "node tools/make-war.js"
  },
  "dependencies": {
    "@pusky/transform-coordinates": "git+https://github.com/PuskyWasTaken/transform-coordinates.git",
    "@terraformer/wkt": "^2.0.7",
    "classnames": "2.2.5",
    "core-js": "2.5.6",
    "flag-icon-css": "3.0.0",
    "isomorphic-fetch": "2.2.1",
    "jsrsasign": "8.0.12",
    "mapbox-gl": "^2.6.1",
    "re-resizable": "^6.9.1",
    "react": "16.13.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.13.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.4.1",
    "tslib": "2.3.0",
    "tv4": "1.3.0",
    "uniqid": "5.0.3"
  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/chai-as-promised": "7.1.0",
    "@types/classnames": "2.2.3",
    "@types/core-js": "0.9.46",
    "@types/enzyme": "3.1.10",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/geojson": "^7946.0.8",
    "@types/history": "4.6.2",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "4.14.108",
    "@types/mapbox-gl": "^2.6.0",
    "@types/mocha": "5.2.0",
    "@types/react": "16.9.23",
    "@types/react-copy-to-clipboard": "4.2.6",
    "@types/react-dom": "16.9.5",
    "@types/react-router": "4.0.25",
    "@types/react-router-dom": "4.2.6",
    "@types/react-test-renderer": "16.0.1",
    "@types/reactstrap": "8.4.2",
    "@types/tv4": "1.2.29",
    "@types/uniqid": "4.1.3",
    "archiver": "2.1.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "git-revision-webpack-plugin": "3.0.3",
